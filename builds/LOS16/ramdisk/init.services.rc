on property:sys.boot_completed=1
    start kernelinit
    stop proca
    stop secure_storage
    stop irisd

service kernelinit /sbin/ts-kernel.sh
    class late_start
    user root
    seclabel u:r:init:s0
    oneshot
    disabled
